subscriptions = {

{% for key in asvm_subscription.subscriptions.keys() %}
  {{ key }} = {
    name                    = "{{ asvm_subscription.subscriptions[key].name }}"
    billing_account_name    = "{{ config_platform.billing_subscription_role_delegations.billing_account_name }}"
    enrollment_account_name = "{{ config_platform.billing_subscription_role_delegations.enrollment_account_name }}"
    management_group_id     = "{{ config_platform.platform_core_setup.enterprise_scale.management_group_prefix }}-{{ asvm_subscription.subscriptions[key].management_group_suffix }}"
    workload                = "{{ asvm_subscription.subscriptions[key].workload | default('Production') }}"
{% if asvm_subscription.subscriptions[key].tags is defined %}
    tags                    = {
{% for tag_key in asvm_subscription.subscriptions[key].tags %}
      {{ tag_key }} = "{{ asvm_subscription.subscriptions[key].tags[tag_key] }}"
{% endfor %}
    }
{% endif %}
  }
{% endfor %}

}